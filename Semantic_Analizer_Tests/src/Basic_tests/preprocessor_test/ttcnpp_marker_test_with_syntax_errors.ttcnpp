testcase tc_cascade() runs on My_CT {
  #define TWENTY 20
  #define TWENTY2 TWENTY
  #define TWENTY3 TWENTY2
  #define TWENTY4 TWENTY3
  #define TWENTY5 TWENTY4
  
  #if TWENTY2 == 20
   {error_marker_expected;}
  #else
   error_marker_not_expected
  #endif  
  
  #if TWENTY5 == 20
   {error_marker_expected;}
  #else
   error_marker_not_expected
  #endif
  
  #if TWENTY5 != 20
   error_marker_not_expected
  #else
   {error_marker_expected;}
  #endif
  
}

