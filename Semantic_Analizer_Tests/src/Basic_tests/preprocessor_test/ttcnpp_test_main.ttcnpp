testcase tc_bitoperands() runs on My_CT{
  #define A 0xFF
  #define B 0x00

  #if (A & B == B)
  setverdict(pass);
  #else
  setverdict(fail);
  #endif

  #if ((A & B) == A)
  setverdict(fail);
  #else
  setverdict(pass);
  #endif

  #if (A|B == B)
  setverdict(pass)
  #else
  setverdict(fail);
  #endif

  #if (A|B == A)
  setverdict(pass)
  #else
  setverdict(fail);
  #endif

  #define A 0x03
  #define B 0x04

  //0000 0011 && 0000 0100
  #if ((A & B) == B)
  setverdict(fail);
  #else
  setverdict(pass);
  #endif

  #if ((A | B)  == A)
  setverdict(fail)
  #else
  setverdict(pass);
  #endif

}

