testcase tc_cascade() runs on My_CT {
  #define TWENTY 20
  #define TWENTY2 TWENTY
  #define TWENTY3 TWENTY2
  #define TWENTY4 TWENTY3
  #define TWENTY5 TWENTY4
  
  #if TWENTY2 == 20
   setverdict(pass)
  #else
   setverdict(fail)
  #endif  
  
  #if TWENTY5 == 20
   setverdict(pass)
  #else
   setverdict(fail)
  #endif
  
  #if TWENTY5 != 20
   setverdict(fail)
  #else
   setverdict(pass)
  #endif
  
}

